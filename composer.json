{
	"name": "dmzx/postingbuttons",
	"type": "phpbb-extension",
	"description": "This extension will add image posting buttons to posting template and all custom BBcodes in a dropdown box",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.2",
	"time": "2020-01-15",
	"keywords": [
		"phpbb",
		"extension",
		"postingbuttons"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
		"extra": {
		"display-name": "Posting Buttons",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0,<4.0.0@dev"
		},
		"version-check": {
			"host": "www.phpbb.com",
			"directory": "/customise/db/extension/posting_buttons",
			"filename": "version_check",
			"ssl": true
		}
	}
}
