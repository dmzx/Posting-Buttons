	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.1",
	"time": "2016-09-19",
	"keywords": ["phpbb", "extension", "postingbuttons"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
		"extra": {
		"display-name": "Posting Buttons",
		"soft-require": {
			"phpbb/phpbb": "3.1.*@dev"
		},
